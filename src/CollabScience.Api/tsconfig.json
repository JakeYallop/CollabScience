{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "ES2016",
    "module": "ES2015",
    "outDir": "wwwroot/js",
    /* Linting */
    "strict": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": ["node_modules", "wwwroot"]
}
